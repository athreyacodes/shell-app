{
  "hosting": {
    "public": "dist/shell-app",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/remoteEntry.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          },
          {
            "key": "Pragma",
            "value": "no-cache"
          },
          {
            "key": "Expires",
            "value": "0"
          }
        ]
      },
      {
        "source": "/remoteEntry.js",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "http://localhost:4200"
          }
        ]
      },
      {
        "source": "/remoteEntry.js",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://shell-app-3d8d6.web.app"
          }
        ]
      },
      {
        "source": "/remoteEntry.js",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://www.athreyamr.dev"
          }
        ]
      }
    ]
  }
}
